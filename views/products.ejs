<% include partials/header %>
<div class="page-header">
<h1>Productos</h1></div>

<div class="row">
        <div class="col-md-12">
        <div class="table-responsive">
<table class="table table-striped table-hover"> 
    <thead>
<tr>
        <th></th>
        <th>Codigo</th>
        <th>Producto</th>
        <th>Precio</th>
</tr>
</thead>
<tbody>
<% products.forEach(function(product){ %>
    <tr>
        <td><a href="/products/<%= product.CODE %>"><span class="glyphicon glyphicon-pencil"></span></a></td>
        <td><%= product.CODE %></td>
        <td><%= product.NAME %></td>
        <td><%= product.PRICESELL %></td>
    </tr>
<% }); %>
</tbody>
</table>
</div></div></div>
<% include partials/footer %>
